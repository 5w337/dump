#include <LedControl.h>
int DIN = 7;
int CLK = 6;
int CS = 5;

byte starandCrescent[] = {
  0b00111110,
  0b01111111,
  0b01000001,
  0b01100011,
  0b00001000,
  0b00011100,
  0b00001000,
  0b00000000
};
LedControl max7219 = LedControl (7,6,5,1);
void setup() {
max7219.shutdown (0, false);
max7219.setIntensity (0, 10);
max7219.clearDisplay (0);
}

void loop() {
for(int shift = 0; shift < 8; shift++) {
  for (int i = 0;i < 8; i++) {
    max7219.setRow(0, i, startandCrescent[(i + shift) % 8]);
}
delay(120);
}
}
